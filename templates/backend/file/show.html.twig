{% extends 'backend.html.twig' %}

{% block title %}File{% endblock %}

{% block body %}
    <h1>File</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ file.id }}</td>
            </tr>
            <tr>
                <th>Original_name</th>
                <td>{{ file.originalName }}</td>
            </tr>
            <tr>
                <th>Ext</th>
                <td>{{ file.ext }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ file.file }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ file.description }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ file.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('backend_file_index') }}" class="btn btn-primary">back to list</a>

    <a href="{{ path('backend_file_edit', {'id': file.id}) }}" class="btn btn-primary">edit</a>

    <div class="pt20">
        {{ include('backend/file/_delete_form.html.twig') }}
    </div>

{% endblock %}
