{% extends 'base.html.twig' %}

{% block title %}Video{% endblock %}

{% block body %}
    {% include 'frontend/header_nav.html.twig' %}
    <ul>
        {% for file in files %}
            <li>
                <video width="320" height="240" controls>
                    <source src="/media/{{ file.file }}" type="video/{{ file.ext }}">
                </video>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
